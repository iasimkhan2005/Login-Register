<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="285.0" prefWidth="410.0" spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab13practice.LoginController">
    <Label prefHeight="23.0" prefWidth="73.0" text="Username:">
      <font>
         <Font name="System Bold" size="14.0" />
      </font></Label>
    <TextField fx:id="usernameField" maxWidth="-Infinity" prefHeight="25.0" prefWidth="283.0" />
    <Label prefHeight="20.0" prefWidth="72.0" text="Password:">
      <font>
         <Font size="14.0" />
      </font></Label>
    <PasswordField fx:id="passwordField" maxWidth="-Infinity" prefHeight="25.0" prefWidth="276.0" />
    <Button onAction="#handleLogin" prefHeight="25.0" prefWidth="110.0" text="Login" />
</VBox>
